{
    "name": "lcfumes/elasticsearch",
    "license": "MIT",
    "type": "project",
    "require": {
        "silex/silex": "~1.3",
        "elasticsearch/elasticsearch": "~2.0",
        "symfony/yaml": "~2.6",
        "twig/twig": "^1.18",
        "twbs/bootstrap": "^3.3"
    },
    "extras": {
